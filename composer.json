{
	"name": "dereuromark/cakephp-codesniffer",
	"type": "cakephp-plugin",
	"description": "A CakePHP to find and correct coding standard issues",
	"keywords": ["cakephp", "plugin", "coding standards", "phpcs", "phpmd", "sniff"],
	"homepage": "https://github.com/dereuromark/cakephp-codesniffer",
	"license": "MIT",
	"authors": [
		{
			"name": "Mark Scherer",
			"homepage": "http://www.dereuromark.de"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4",
		"phpmd/phpmd": "2.*",
		"squizlabs/php_codesniffer": "2.*",
		"cakephp/cakephp": "3.0.x-dev",
		"cakephp/plugin-installer": "dev-master",
		"dereuromark/codesniffer-standards": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"CodeSniffer\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeSniffer\\Test\\": "tests"
		}
	},
	"support": {
		"source": "https://github.com/dereuromark/cakephp-codesniffer"
	},
	"extra": {
		"installer-name": "CodeSniffer"
	}
}
